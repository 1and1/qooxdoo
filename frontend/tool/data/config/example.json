{
  "common" :
  {
    "library" :
    [
      {
        "encoding" : "utf-8",
        "path" : "framework/source",
        "uri" : "../../../framework/source"
      },

      {
        "encoding" : "utf-8",
        "path" : "application/apiviewer/source",
        "uri" : "../source"
      },

      {
        "encoding" : "utf-8",
        "path" : "application/feedreader/source",
        "uri" : "../source"
      },

      {
        "encoding" : "utf-8",
        "path" : "application/showcase/source",
        "uri" : "../source"
      }
    ],

    "cache" :
    {
      "path" : "cache"
    }
  },





  "compile-bom" :
  {
    "extend" : ["common"],

    "include" : ["qx.bom.Element"],

    "variants" :
    {
      "qx.debug" : ["off"],
      "qx.aspects" : ["off"],
      "qx.client" : ["gecko","mshtml"]
    },

    "compile" :
    {
      "file" : "compile-bom-{qx.client}.js",
      "optimize" : [ "variables", "basecalls", "privates" ]
    }
  },




  "source-all" :
  {
    "extend" : ["common"],

    "source" :
    {
      "file" : "source-all.js"
    }
  },

  "compile-all" :
  {
    "extend" : ["common"],

    "compile" :
    {
      "file" : "build-all.js"
    }
  },

  "localize-all" :
  {
    "extend" : ["common"],

    "localize" :
    {
      "locales" : ["de", "en", "fr"]
    }
  },







  "api-feedreader" :
  {
    "extend" : ["common"],
    "include" : ["feedreader.Application"],

    "api" :
    {
      "path" : "application/apiviewer/source/script"
    }
  },

  "po-feedreader" :
  {
    "extend" : ["common"],

    "translation" :
    {
      "update" : ["feedreader"]
    }
  },

  "source-feedreader" :
  {
    "extend" : ["common"],
    "include" : ["feedreader.*", "qx.legacy.theme.Ext"],

    "settings" :
    {
      "qx.application" : "feedreader.Application",
      "qx.resourceUri" : "../../../framework/source/resource",
      "qx.legacy.theme" : "qx.legacy.theme.Ext"
    },

    "source" :
    {
      "file" : "feedreader-source.js",
      "gzip" : "true"
    },

    "localize" :
    {
      "locales" : ["en", "de", "es", "it"]
    }
  },

  "source-feedreader-parts" :
  {
    "extend" : ["source-feedreader"],

    "packages" :
    {
      "size" : 5,

      "parts" :
      {
        "core" : ["feedreader.Application"],
        "table" : ["feedreader.view.Table"],
        "tree" : ["feedreader.view.Tree"]
      }
    },

    "localize" :
    {
      "locales" : ["en", "de", "es", "it"]
    }
  },

  "compile-feedreader" :
  {
    "extend" : ["common"],
    "include" : ["feedreader.*", "qx.legacy.theme.Ext"],

    "settings" :
    {
      "qx.application" : "feedreader.Application",
      "qx.resourceUri" : "resource/qx",
      "qx.legacy.theme" : "qx.legacy.theme.Ext",
      "feedreader.resourceUri" : "resource/feedreader"
    },

    "variants" :
    {
      "qx.debug" : ["off"]
    },

    "compile" :
    {
      "file" : "application/feedreader/build/script/feedreader.js",
      "uri" : "script/feedreader.js",
      "optimize" : [ "variables", "basecalls", "privates" ]
    }
  },

  "compile-feedreader-parts" :
  {
    "extend" : ["compile-feedreader"],

    "packages" :
    {
      "size" : 5,

      "parts" :
      {
        "core" : ["feedreader.Application"],
        "table" : ["feedreader.view.Table"],
        "tree" : ["feedreader.view.Tree"]
      }
    }
  },






  "api-apiviewer" :
  {
    "extend" : ["common"],
    "include" : ["apiviewer.Application"],

    "api" :
    {
      "path" : "application/apiviewer/source/script"
    }
  },

  "source-apiviewer" :
  {
    "extend" :["common"],
    "include" : ["apiviewer.Application", "qx.theme.ClassicRoyale"],

    "settings" :
    {
      "qx.theme" : "qx.theme.ClassicRoyale",
      "qx.application" : "apiviewer.Application",
      "qx.resourceUri" : "../../../framework/source/resource",
      "apiviewer.title" : "API Viewer (g2)"
    },

    "source" :
    {
      "file" : "application/apiviewer/source/script/apiviewer.js"
    }
  },

  "compile-apiviewer" :
  {
    "extend" :["common"],
    "include" : ["apiviewer.Application", "qx.theme.ClassicRoyale"],

    "settings" :
    {
      "qx.theme" : "qx.theme.ClassicRoyale",
      "qx.application" : "apiviewer.Application",
      "qx.resourceUri" : "../../../framework/source/resource",
      "apiviewer.title" : "API Viewer (g2)"
    },

    "variants" :
    {
      "qx.debug" : ["off"]
    },

    "compile" :
    {
      "file" : "application/apiviewer/build/script/apiviewer.js"
    }
  }
}
